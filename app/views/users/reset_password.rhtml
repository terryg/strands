<% form_for :user do |f| -%>
<p><label for="email">Password</label><br/>
<%= f.password_field :password %></p>

<p><label for="email">Confirm Password</label><br/>
<%= f.password_field :password_confirmation %></p>
<%= hidden_field_tag :reset_code, params[:reset_code] %>
<p><%= submit_tag 'Reset Password' %></p>
<% end -%>
